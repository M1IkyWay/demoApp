<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cashluckpatrol"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="26"
        android:targetSdkVersion="33" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />

    <permission
        android:name="com.example.cashluckpatrol.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.cashluckpatrol.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.CashLuckPatrol" >
        <activity
            android:name="com.example.cashluckpatrol.SlotGame2Activity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.SlotGame1Activity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.SplashScreen"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cashluckpatrol.FlashGame2Activity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.FlashGame1Activity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.ComingSoonActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.HotGameActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.GamesMenuActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.GamesMenuAnonActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.SettingsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.ChoosingModeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.AgreementActivity"
            android:exported="false" />
        <activity
            android:name="com.example.cashluckpatrol.PrivacyPolicy"
            android:exported="false"
            android:label="@string/title_activity_privacy_policy"
            android:theme="@style/Theme.CashLuckPatrol.NoActionBar" />
        <activity
            android:name="com.example.cashluckpatrol.PlayActivity"
            android:exported="false" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.cashluckpatrol.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>